
INIT_VIDEO()
VIDEO_SET_COLOR([1/2, 1, 1/2])
VIDEO_CLEAR()
VIDEO_PRESENT()
EventLoop()

fn EventLoop
    while true
        for [type, *args] in POLL()
            if type == 'Quit'
                return
            else
                print [type, args]
            end
        end
        SLEEP(1 / 60)
    end
end
