import a.gui

gui.Init()

Main()

fn Main
    while true
        for [type, *args] in gui.Poll()
            if type == 'Key'
                [key] = args
                if key == 'Escape'
                    return
                else
                    print ['key', key]
                end
            else
                print [type, args]
            end
        end
        SLEEP(1 / 30)
    end
end
