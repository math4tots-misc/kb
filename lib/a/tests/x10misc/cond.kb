
fn[test] ConditionalExpression
    x = if 1 then 'a' else 'b'
    assert x == 'a'
    x = if 0 then 'a' else 'b'
    assert x == 'b'

    # Chained
    y = 10
    x = (
        if y < 10 then 'y lt 10' else
        if y > 10 then 'y gt 10' else
        'y eq 10'
    )
    assert x == 'y eq 10'

    y = 5
    x = (
        if y < 10 then 'y lt 10' else
        if y > 10 then 'y gt 10' else
        'y eq 10'
    )
    assert x == 'y lt 10'

    y = 15
    x = (
        if y < 10 then 'y lt 10' else
        if y > 10 then 'y gt 10' else
        'y eq 10'
    )
    assert x == 'y gt 10'
end

fn[test] AndOr
    assert (0 and 1) == 0
    assert (2 and 5) == 5
    assert (1 and 0) == 0

    assert (0 or 1) == 1
    assert (2 or 5) == 2
    assert (1 or 0) == 1
end

fn[test] Not
    assert (not true) == false
    assert (not 0) == true
    assert (not 1) == false
end
