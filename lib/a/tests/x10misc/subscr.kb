

fn[test] StringGetItem
    s = 'abc'
    assert s[0] == 'a'
    assert s[1] == 'b'
    assert s[2] == 'c'
end


fn[test] ListGetItem
    list = ['aaa', 22, 55]
    assert list[0] == 'aaa'
    assert list[1] == 22
    assert list[2] == 55
end

fn[test] ListSetItem
    list = ['aaa', 22, 55]
    assert list == ['aaa', 22, 55]
    list[1] = 'foo'
    assert list != ['aaa', 22, 55]
    assert list == ['aaa', 'foo', 55]
end
