TEST Try
    # catch case
    passed_catch = false
    try
        1 + 'hi'
    catch e
        passed_catch = true
    end
    assert passed_catch

    # no catch case
    passed_catch = false
    try
    catch e
        passed_catch = true
    end
    assert not passed_catch
end

TEST ThrowDirect
    try
        throw 10
    catch e
    end
    assert e == 10
end
