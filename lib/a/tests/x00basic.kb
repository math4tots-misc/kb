

fn[test] Sanity
    assert true
    assert 1 != 2
    assert 1 == 1
    assert 'abc' == 'abc'

    assert true is true
    assert true is not false
    assert 1 is 1
    assert 1 is not 2
end

fn[test] GotoLineNumber
    i = 0
    total = 0

    10 total = total + i
    20 i = i + 1
    if i < 10
        GOTO 10
    end

    assert total == 45
end

fn[test] StringLiterals
    x = r"hi"
    assert x == 'hi'
end

fn[test] NotEqual
    assert 1 <> 2
end
